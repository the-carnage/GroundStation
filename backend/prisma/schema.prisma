// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}

model OtaData {
  id          Int      @id @default(autoincrement())
  value       Int
  voltage     Float
  createdAt   DateTime @default(now())
}

model Imu6050Data {
  id        Int      @id @default(autoincrement())
  accelX    Float
  accelY    Float
  accelZ    Float
  gyroX     Float
  gyroY     Float
  gyroZ     Float
  createdAt DateTime @default(now())
}

model Bmp280Data {
  id          Int      @id @default(autoincrement())
  pressure    Float
  temperature Float
  createdAt   DateTime @default(now())
}

model GpsData {
  id         Int      @id @default(autoincrement())
  latitude   Float
  longitude  Float
  satellites Int
  createdAt  DateTime @default(now())
}

model BatteryData {
  id         Int      @id @default(autoincrement())
  voltage    Float
  current    Float
  percentage Int
  createdAt  DateTime @default(now())
}

model EscData {
  id        Int      @id @default(autoincrement())
  esc1Rpm   Int
  esc2Rpm   Int
  esc3Rpm   Int
  esc4Rpm   Int
  createdAt DateTime @default(now())
}

model ContourData {
  id        Int      @id @default(autoincrement())
  xValue    Float
  yValue    Float
  zValue    Float
  createdAt DateTime @default(now())
}