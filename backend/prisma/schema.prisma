// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}

model hw233Ota {
  id          Int      @id @default(autoincrement())
  ota_value   Int
  ota_voltage Float
  createdAt   DateTime @default(now())
}
